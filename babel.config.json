{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": true
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "#root": "./",
        "#src": "./src",
        "#mocks": "./test-settings/__mocks__/"
      }
    }]
  ]
}